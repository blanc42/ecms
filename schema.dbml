Table admin {
  id int [pk, increment]
  username varchar(255) [not null, unique]
  password varchar(255) [not null]
  email varchar(255) [not null, unique]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table store {
  id int [pk, increment]
  name varchar(255) [not null]
  description text
  admin_id int [not null, ref: > admin.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table category {
  id int [pk, increment]
  name varchar(255) [not null]
  description text
  store_id int [not null, ref: > store.id]
  parent_category_id int [ref: > category.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table product {
  id int [pk, increment]
  name varchar(255) [not null]
  description text
  rating float
  is_featured bool
  is_archived bool
  has_variants bool
  category_id int [not null, ref: > category.id]
  store_id int [not null, ref: > store.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table variant {
  id int [pk, increment]
  name varchar(255) [not null]
  description text
  weight int [not null]
  category_id int [not null, ref: > category.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table variant_option {
  id int [pk, increment]
  value varchar(255) [not null]
  description text
  weight int
  variant_id int [not null, ref: > variant.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table product_item {
  id int [pk, increment]
  product_id int [not null, ref: > product.id]
  sku varchar(255) [not null, unique]
  quantity int [not null]
  price float [not null]
  discounted_price float
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table product_image {
  id int [pk, increment]
  product_id int [not null, ref: > product.id]
  image_url varchar(255) [not null]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table customer {
  id int [pk, increment]
  username varchar(255) [not null, unique]
  password varchar(255) [not null]
  email varchar(255) [not null, unique]
  store_id int [not null, ref: > store.id]
  address_id int [ref: > address.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table order {
  id int [pk, increment]
  order_number varchar(255) [not null, unique]
  payment_status varchar(50) [not null]
  order_status varchar(50) [not null]
  store_id int [not null, ref: > store.id]
  customer_id int [not null, ref: > customer.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table order_item {
  id int [pk, increment]
  product_item_id int [not null, ref: > product_item.id]
  quantity int [not null]
  order_id int [not null, ref: > order.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table cart {
  id int [pk, increment]
  customer_id int [not null, ref: > customer.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table cart_item {
  id int [pk, increment]
  product_item_id int [not null, ref: > product_item.id]
  quantity int [not null]
  cart_id int [not null, ref: > cart.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table address {
  id int [pk, increment]
  city varchar(255) [not null]
  pincode varchar(255) [not null]
  country_id int [not null, ref: > country.id]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

Table country {
  id int [pk, increment]
  country varchar(255) [not null]
  created_at timestamp
  updated_at timestamp
  deleted_at timestamp
}

// Relationships are defined within the table definitions above